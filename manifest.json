{
    "manifest_version": 2,
    "name": "BOT-LIKE-INSTAGRAM",
    "description": "BOT-LIKE-INSTAGRAM",
    "version": "1.0",
    "icons": { "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" },
    "content_scripts": [
        {
            "matches": [
                "https://*.instagram.com/*"
            ],
            "js": [
                "index.js"
            ],
            "css":[
                "style.css"
            ]
        }
    ]
}